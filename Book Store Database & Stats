create table books (id INTEGER PRIMARY KEY, item TEXT, price INTEGER, quantity INTEGER );
INSERT INTO books VALUES (1, "INUYASHA", 9.99, 5);
INSERT INTO books VALUES (2, "DGRAYMAN",7.99, 3);
